<div>
  管理者ログイン画面
  <%= form_for :session, url: admin_administrator_login_path  do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :mail%>
    <%= f.email_field :mail %>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.label :confirmation %>
    <%= f.text_field :password %>
    <%= button_to "ログイン" %>
  <% end %>
</div>