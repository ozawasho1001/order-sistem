<table>
  <tr>
    <th>ユーザーID</th>
    <th>数量</th>
    <th>商品名</th>
    <th>日時</th>
    <th>削除</th>
  </tr>

  <% @order.each do |order| %>
    <tr>
      <td><%= order.user_id%></td>
      <td><%= order.quantity %></td>
      <td><%= order.cuisine_name %></td>
      <td><%= order.created_at %></td>
      <td><%= link_to "削除", "/", method: :delete %></td>
    </tr>
  <% end %>
</table>