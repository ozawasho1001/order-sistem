<div id="product_list">
  <% @cuisines.each do |cuisine| %>
    <div class="card">
      <%= image_tag cuisine.image.url, class: "product_image" %>
      <div>
        <%= cuisine.name %>
      </div>
      <div>
        <%= cuisine.text  %>
      </div>
      <div>
        <%= cuisine.price  %>
      </div>
    </div>
  <% end %>
</div>